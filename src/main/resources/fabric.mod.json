{
  "schemaVersion": 1,
  "id": "abnormalib",
  "version": "%VERSION%",

  "name": "AbnormaLib",
  "description": "AbnormaLib is a library mod for all mods made by Minecraft Abnormals",
  "authors": [
    "Team Abnormals"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/minecraftabnormals/AbnormaLib-1.14"
  },

  "license": "MIT",
  "icon": "assets/abnormalib/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "team.abnormals.abnormalib.AbnormaLib",
      "team.abnormals.module_api.ModuleManager::init"
    ],
    "client": [
      "team.abnormals.module_api.ModuleManager::initClient"
    ],
    "modmenu": [
      "team.abnormals.module_api.ModuleManager$ModMenuIntegration"
    ]
  },
  "mixins": [
    "abnormalib.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": ">=0.3.0"
  }
}
